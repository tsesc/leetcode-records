   "source": [
    "class Solution:\n",
    "    def isPalindrome(self, s: str) -> bool:\n",
    "    \ts = [i for i in s.lower() if i.isalnum()]\n",
    "        \n",
    "    \treturn s == s[::-1]\n",
    "\n"
   ]
